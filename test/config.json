{
    "TopNPaths": 10,
    "IgnoredNamespaces": [
        "/^-$/",
        "test"
    ],
    "IgnoredRouters": [
        "website",
        "api@internal",
        "prometheus@internal",
        "http-to-443@internal"
    ],
    "IgnoredPathsRegex": [
        "\\/images\\/",
        "\\/metrics\\/"
    ],
    "MergePathsWithExtensions": [
        "/api",
        "/test"
    ],
    "URLPatterns": [
        {
            "service_name": "jsonapi",
            "pattern": "^/api/v1/products/([a-zA-Z0-9\\-]+)$",
            "name": "/api/v1/products/{name}"
        }
    ],
    "AllowedServices": [
        "hikmah-api",
        "mahfil-api"
    ]
}